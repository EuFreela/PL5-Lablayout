{
    "name": "lameck/lablayout",
    "description": "\"Lameck Backend Layout Pck\"",
    "keywords": [
        "laravel",
        "packager",
        "layout",
        "lameck",
        "backend"
    ],
    "homepage": "https://github.com/EuFreela/PL5-Lablayout",
    "license": "MIT",
    "authors": [
        {
            "name": "Lameck Fernandes",
            "email": "lameckfernandes@hotmail.com",
            "homepage": "https://github.com/EuFreela",
            "role": "Developer"
        }
    ],
    "require-dev": {       
        "php": ">=7.0"
    },
    "autoload": {
        "psr-4": {
            "Lameck\\Lablayout\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Lameck\\Lablayout\\LablayoutServiceProvider"
            ]
        }
    }, 
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "stable",    
    "prefer-stable": true    
}